<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>TechniLux</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>2.6.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<true/>
	</dict>
	<key>UILaunchScreen</key>
	<dict>
		<key>UIColorName</key>
		<string>LaunchScreenBackground</string>
		<key>UIImageName</key>
		<string>LaunchIcon</string>
	</dict>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>NSFaceIDUsageDescription</key>
	<string>TechniLux uses Face ID to securely authenticate and access your DNS server.</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.technilux.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>technilux</string>
			</array>
		</dict>
	</array>
	<key>NSSupportsLiveActivities</key>
	<true/>
	<key>UIApplicationShortcutItems</key>
	<array>
		<dict>
			<key>UIApplicationShortcutItemType</key>
			<string>com.technilux.toggleBlocking</string>
			<key>UIApplicationShortcutItemTitle</key>
			<string>Toggle Blocking</string>
			<key>UIApplicationShortcutItemSubtitle</key>
			<string>Enable or disable DNS blocking</string>
			<key>UIApplicationShortcutItemIconSymbolName</key>
			<string>shield.checkmark</string>
		</dict>
		<dict>
			<key>UIApplicationShortcutItemType</key>
			<string>com.technilux.quickDisable</string>
			<key>UIApplicationShortcutItemTitle</key>
			<string>Disable 5 Minutes</string>
			<key>UIApplicationShortcutItemSubtitle</key>
			<string>Temporarily disable blocking</string>
			<key>UIApplicationShortcutItemIconSymbolName</key>
			<string>clock.badge.xmark</string>
		</dict>
		<dict>
			<key>UIApplicationShortcutItemType</key>
			<string>com.technilux.dashboard</string>
			<key>UIApplicationShortcutItemTitle</key>
			<string>Dashboard</string>
			<key>UIApplicationShortcutItemSubtitle</key>
			<string>View DNS statistics</string>
			<key>UIApplicationShortcutItemIconSymbolName</key>
			<string>chart.bar</string>
		</dict>
		<dict>
			<key>UIApplicationShortcutItemType</key>
			<string>com.technilux.logs</string>
			<key>UIApplicationShortcutItemTitle</key>
			<string>Query Logs</string>
			<key>UIApplicationShortcutItemSubtitle</key>
			<string>View DNS query logs</string>
			<key>UIApplicationShortcutItemIconSymbolName</key>
			<string>doc.text</string>
		</dict>
		<dict>
			<key>UIApplicationShortcutItemType</key>
			<string>com.technilux.toggleAdvancedBlocking</string>
			<key>UIApplicationShortcutItemTitle</key>
			<string>Toggle Adv. Blocking</string>
			<key>UIApplicationShortcutItemSubtitle</key>
			<string>Enable/disable Advanced Blocking</string>
			<key>UIApplicationShortcutItemIconSymbolName</key>
			<string>hand.raised.slash</string>
		</dict>
	</array>
</dict>
</plist>
